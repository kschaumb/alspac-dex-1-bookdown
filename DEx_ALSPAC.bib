@article{ubanBiospecimensABCDStudy2018,
  title = {Biospecimens and the {{ABCD}} Study: {{Rationale}}, Methods of Collection, Measurement and Early Data},
  shorttitle = {Biospecimens and the {{ABCD}} Study},
  author = {Uban, Kristina A. and Horton, Megan K. and Jacobus, Joanna and Heyser, Charles and Thompson, Wesley K. and Tapert, Susan F. and Madden, Pamela A.F. and Sowell, Elizabeth R.},
  year = {2018},
  month = aug,
  journal = {Developmental Cognitive Neuroscience},
  volume = {32},
  pages = {97--106},
  issn = {18789293},
  doi = {10.1016/j.dcn.2018.03.005},
  abstract = {Biospecimen collection in the Adolescent Brain Cognitive Development (ABCD) study \textendash{} of hair samples, shed deciduous (baby) teeth, and body fluids \textendash{} will serve dual functions of screening for study eligibility, and providing measures of biological processes thought to predict or correlate with key study outcomes on brain and cognitive development. Biosamples are being collected annually to screen for recency of drug use prior to the neuroimaging or cognitive testing visit, and to store for the following future studies: (1) on the effects of exposure to illicit and recreational drugs (including alcohol and nicotine); (2) of pubertal hormones on brain and cognitive developmental trajectories; (3) on the contribution of genomics and epigenomics to child and adolescent development and behavioral outcomes; and (4) with pre- and post-natal exposure to environmental neurotoxicants and drugs of abuse measured from novel tooth analyses. The present manuscript describes the rationales for inclusion and selection of the specific biospecimens, methodological considerations for each measure, future plans for assessment of biospecimens during follow-up visits, and preliminary ABCD data to illustrate methodological considerations.},
  langid = {english},
  file = {/Users/kschaumb/Zotero/storage/GW65PNWR/Uban et al. - 2018 - Biospecimens and the ABCD study Rationale, method.pdf}
}


