  title = {Cohort {{Profile}}: {{The}} 'Children of the 90s'\textendash the Index Offspring of the {{Avon Longitudinal Study}} of {{Parents}} and {{Children}}.},
  author = {Boyd, A. and Golding, J. and Macleod, J. and Lawlor, D. A. and Fraser, A. and Henderson, J. and Molloy, L. and Ness, A. and Ring, S. and Davey Smith, G.},
  year = {2013},
